{
  "defaultSeverity": "warning",
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-consecutive-blank-lines": { "severity": "off" },
    "no-submodule-imports": [true, "bootstrap", "react-bootstrap-typeahead"]
  },
  "linterOptions": {
    "exclude": [
      "vendor/**/*.ts",
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/**/*.js",
      "src/**/*.jsx"
    ]
  }
}
